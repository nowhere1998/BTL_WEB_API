<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0, user-scalable=no, target-densityDpi=device-dpi" />
    <title>ADMIN LOGIN</title>
    <link rel="icon" type="image/png" href="/user/assets/images/favicon.png">
    <link rel="stylesheet" href="/user/assets/css/all.min.css">
    <link rel="stylesheet" href="/user/assets/css/bootstrap.min.css">
    <link rel="stylesheet" href="/user/assets/css/nice-select.css">
    <link rel="stylesheet" href="/user/assets/css/slick.css">
    <link rel="stylesheet" href="/user/assets/css/venobox.min.css">
    <link rel="stylesheet" href="/user/assets/css/ranger_slider.css">
    <link rel="stylesheet" href="/user/assets/css/animate.css">
    <link rel="stylesheet" href="/user/assets/css/scroll_button.css">
    <link rel="stylesheet" href="/user/assets/css/custom_spacing.css">
    <link rel="stylesheet" href="/user/assets/css/select2.min.css">
    <link rel="stylesheet" href="/user/assets/css/colorfulTab.min.css">
    <link rel="stylesheet" href="/user/assets/css/jquery.animatedheadline.css">
    <link rel="stylesheet" href="/user/assets/css/animated_barfiller.css">
    <link rel="stylesheet" href="/user/assets/css/style.css">
    <link rel="stylesheet" href="/user/assets/css/responsive.css">
</head>
<body>
    <section class="sign_in">
        <div class="row justify-content-center">
            <div class="col-xl-6 col-lg-6 wow fadeInLeft">
                <div class="sign_in_img">
                    <img src="/user/assets/images/sign_in_img.jpg" alt="sign in" class="img-fluid w-100">
                </div>
            </div>
            <div class="col-xl-6 col-md-10 col-lg-6 wow fadeInRight">
                <div class="sign_in_form">
                    <a class='sign_in_logo' href='index.html'>
                        <img src="/user/assets/images/logo.png" alt="sign in" class="img-fluid w-100">
                    </a>
                    <h2>sign in admin</h2>
                    @Html.Raw(ViewBag.error)
                    <form asp-action="login" method="post">
                        <div class="row">
                            <div class="col-md-6">
                                <input type="text" placeholder="Enter your email..." name="email" value="@ViewBag.Email">
                            </div>
                            <div class="col-md-6">
                                <input type="password" placeholder="Enter your password..." name="password" value="@ViewBag.Password">
                            </div>
                            <div class="col-md-12">
                                <div class="sign_in_check_area">
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" value="" id="flexCheckDefault">
                                        <label class="form-check-label" for="flexCheckDefault">
                                            Remember me
                                        </label>
                                    </div>
                                    <a href='forgot_password.html'>Forget Password?</a>
                                </div>
                            </div>
                            <div class="col-12">
                                <button class="common_btn" type="submit">sign in</button>
                            </div>
                        </div>
                    </form>
                    <p class="or">or</p>
                    <ul>
                        <li>
                            <a href="#">
                                <span>
                                    <img src="/user/assets/images/google_icon.svg" alt="google" class="img-fluid w-100">
                                </span>
                                Google
                            </a>
                        </li>
                        <li>
                            <a href="#">
                                <span>
                                    <img src="/user/assets/images/fb_icon.svg" alt="facebook" class="img-fluid w-100">
                                </span>
                                facebook
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </section>
    </footer>
    <!--==========================
        FOOTER END
    ===========================-->
    <!--==========================
        SCROLL BUTTON START
    ===========================-->
    <div class="progress-wrap">
        <svg class="progress-circle svg-content" width="100%" height="100%" viewBox="-1 -1 102 102">
            <path d="M50,1 a49,49 0 0,1 0,98 a49,49 0 0,1 0,-98" />
        </svg>
    </div>
    <!--==========================
        SCROLL BUTTON END
    ===========================-->
    <!--jquery library js-->
    <script src="/user/assets/js/jquery-3.7.1.min.js"></script>
    <!--bootstrap js-->
    <script src="/user/assets/js/bootstrap.bundle.min.js"></script>
    <!--font-awesome js-->
    <script src="/user/assets/js/Font-Awesome.js"></script>
    <!--nice select js-->
    <script src="/user/assets/js/jquery.nice-select.min.js"></script>
    <!--marquee js-->
    <script src="/user/assets/js/jquery.marquee.min.js"></script>
    <!--slick js-->
    <script src="/user/assets/js/slick.min.js"></script>
    <!--countup js-->
    <script src="/user/assets/js/jquery.waypoints.min.js"></script>
    <script src="/user/assets/js/jquery.countup.min.js"></script>
    <!--venobox js-->
    <script src="/user/assets/js/venobox.min.js"></script>
    <!--scroll button js-->
    <script src="/user/assets/js/scroll_button.js"></script>
    <!--price ranger js-->
    <script src="/user/assets/js/ranger_jquery-ui.min.js"></script>
    <script src="/user/assets/js/ranger_slider.js"></script>
    <!--select 2 js-->
    <script src="/user/assets/js/select2.min.js"></script>
    <!--aos js-->
    <script src="/user/assets/js/wow.min.js"></script>
    <!--colorfulTab js-->
    <script src="/user/assets/js/colorfulTab.min.js"></script>
    <!--sticky sidebar js-->
    <script src="/user/assets/js/sticky_sidebar.js"></script>
    <!--animated barfiller js-->
    <script src="/user/assets/js/animated_barfiller.js"></script>
    <!--animatedheadline js-->
    <script src="/user/assets/js/jquery.animatedheadline.min.js"></script>
    <!--script/custom js-->
    <script src="/user/assets/js/script.js"></script>
</body>


